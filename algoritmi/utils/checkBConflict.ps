
function checkBConflict(instance, vehicleA, vehicleB) {
    assert(vehicleA != vehicleB)
    assert(1 <= vehicleA <= instance.size)
    assert(1 <= vehicleB <= instance.size)
    if (vehicleA > vehicleB) {
        return checkBConflict(instance, vehicleB, vehicleA)
    }
    assert(vehicleA < vehicleB)

    set sigma = instance.sigma
    return (
        (vehicleB - vehicleA) % 2 == 0 &&
        vehicleA in instance.R && vehicleB in instance.L &&
        sigma(vehicleB) < (vehicleA + vehicleB)/2 < sigma(vehicleA)
    )
}
