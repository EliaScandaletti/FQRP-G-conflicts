
function isNotPartitioned(instance) {
    return !isPartitioned(instance)
}

function isPartitioned(instance) {
    set sigma = instance.sigma

    set i = 0
    set maxSigma = 1
    while (i < instance.size && maxSigma > i) { 
        i += 1
        maxSigma = max(maxSigma, sigma(i))
    }
    return i < instance.size
}