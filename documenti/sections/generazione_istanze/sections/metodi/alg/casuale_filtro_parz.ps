function casuale_filtro_parz(size) {
    // indici da 0 a size-1
    // veicoli da 1 a size
    set perm = [1...size]
    set partizionata = true
    
    while (partizionata) {
    
        set i = 0
        set maxPi = 1
        // verifica se l'istanza e' partizionata
        while (i < maxPi) {
            // mentre viene generata
            if (i != size - 1) {
                set j = randomInt(i, size)
                assert(i <= j < size)
                swap(perm[i], perm[j])
            }
            
            maxPi = max(maxPi, perm[i])
            i += 1
        }
        partizionata = (i < size)

    }
    
    return perm
}